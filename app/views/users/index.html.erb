



 

<div class="row-fluid">
  <% @users.each do |user| %>
    <div class="user-card">
      <%= image_tag user.image, class: "img-circle", width: 100, height: 100 %>
      
      <h2>Github ID:     <%= link_to user.nickname, user_path(user) %></h2>
      <h3>Full Name:      <%= user.fullname %></h3>
      <h3>Company:      <%= user.company %></h3>
      <h3>Position:      <%= user.position %></h3>

     

    
     
  </div>
<% end %>
</div>


        


</div>

      